<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/default_color">
    <item
        android:id="@+id/backgroundTemporary"
        android:bottom="2dp"
        android:left="3dp"
        android:right="3dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/dark_transparent" />
            <corners
                android:radius="7dp"/>
        </shape>
    </item>
    <item
        android:id="@android:id/mask"
        android:bottom="2dp"
        android:left="3dp"
        android:right="3dp"
        android:top="2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/default_color_transparent"/>
            <corners android:radius="7dp"/>
        </shape>
    </item>
</ripple>